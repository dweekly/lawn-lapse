# UniFi Protect Configuration Example
# Copy this file to .env.local and fill in your values
# Run 'node setup.js' for interactive configuration

# UniFi Protect host or IP address (no protocol)
UNIFI_HOST=192.168.1.1

# UniFi Protect credentials
UNIFI_USERNAME=your_username_here
UNIFI_PASSWORD=your_password_here

# Camera configuration
CAMERA_ID=your_camera_id_here
CAMERA_NAME=Front Yard

# Output directory for snapshots
OUTPUT_DIR=./snapshots

# Capture time configuration (24-hour format)
CAPTURE_HOUR=12
CAPTURE_MINUTE=0